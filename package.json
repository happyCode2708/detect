{
  "name": "foodocr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "heroku-postbuild": "npm run build",
    "lint": "next lint",
    "custom-dev-local": "source ~/.bash_python && nodemon",
    "build:server": "tsc --project tsconfig.server.json",
    "build:custom-next": "cross-env NEXT_BUILD=true",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,py}\" dist/",
    "build:next": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NEXT_BUILD=true node dist/server.js",
    "build": "cross-env NODE_ENV=production yarn build:server && yarn copyfiles && yarn build:next"
  },
  "dependencies": {
    "@google-cloud/vertexai": "^1.1.0",
    "@google-cloud/vision": "^4.2.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.32.1",
    "axios": "^1.6.8",
    "cheerio": "^1.0.0-rc.12",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.377.0",
    "multer": "^1.4.5-lts.1",
    "next": "14.2.3",
    "nodemon": "^3.1.0",
    "openai": "^4.40.0",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.33.3",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "copyfiles": "^2.4.1",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": "18.17.0"
  }
}
